{
  "headers": [
    {
      "source" : "**/*.js",
      "headers" : [
        {
          "key" : "Cache-Control",
          "value" : "no-cache"
        }
      ]
    },


    {
      "source" : "**/*.html",
      "headers" : [
        {
          "key" : "Cache-Control",
          "value" : "no-cache"
        },
        {
          "key": "Cross-Origin-Embedder-Policy",
          "value": "require-corp"
        }, {
          "key": "Cross-Origin-Opener-Policy",
          "value": "same-origin"
        }
      ]
    }

  ]
}
